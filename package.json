    "dx:pls": "sfdx force:source:retrieve -m FlexiPage",
    "dx:msg": "sfdx force:source:retrieve -m CustomObject:Message__c",
    "dx:msgFs": "sfdx force:source:retrieve -m CustomField:Message__c",
    "dx:retrieve": "sfdx force:source:retrieve -m LightningComponentBundle -u lukeDev"
